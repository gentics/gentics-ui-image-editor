{
  "name": "gentics-image-editor",
  "version": "0.1.0",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n gentics-image-editor -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "webpack --config playground/webpack.config.js -w -d",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repo"
  },
  "author": {
    "name": "Michael Bromley",
    "email": "m.bromley@gentics.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/username/repo/issues"
  },
  "dependencies": {
    "cropperjs": "^1.3.2",
    "gentics-ui-core": "^6.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.8",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.2.8",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.2.8",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "2.5.53",
    "@types/node": "~6.0.60",
    "angular-in-memory-web-api": "^0.3.2",
    "codelyzer": "~3.2.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.10",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
