    "run-sequence": "^1.2.2",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "systemjs": "^0.20.12",
    "systemjs-plugin-babel": "0.0.25",
    "ts-loader": "^4.0.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
